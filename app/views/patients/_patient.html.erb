<div id="<%= dom_id patient %>">
  <p>
    <strong>Mrn:</strong>
    <%= patient.mrn %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= patient.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= patient.last_name %>
  </p>

  <p>
    <strong>Middle initial:</strong>
    <%= patient.middle_initial %>
  </p>

  <p>
    <strong>Date of birth:</strong>
    <%= patient.date_of_birth %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= patient.phone %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= patient.email %>
  </p>

  <p>
    <strong>Deceased:</strong>
    <%= patient.deceased %>
  </p>

  <p>
    <strong>Terminated:</strong>
    <%= patient.terminated %>
  </p>

  <p>
    <strong>Language:</strong>
    <%= patient.language_id %>
  </p>

  <p>
    <strong>Sex:</strong>
    <%= patient.sex_id %>
  </p>

</div>
